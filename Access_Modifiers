package inheritancesDemo;

public class AccessModifier1 {
	
	int hours =3;
	int mins = 47;
	
	public String name = "josh";
	public String tool = "Selenium";
	
	private int a =3;
	private int b = 47;
	
	protected int x = 100;
	protected int z = 200;
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}

package inheritancesDemo;

public class AccessModifiers2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		AccessModifier1 obj = new AccessModifier1();
		
		System.out.println(obj.hours);
		System.out.println(	obj.mins);
		
		
		
		System.out.println(obj.name);
		System.out.println(obj.tool);
		
		
		System.out.println(obj.x);
		System.out.println(obj.z);

	}

}
